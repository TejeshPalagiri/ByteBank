{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/server.js",
            "preLaunchTask": "npm: build",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "runtime    Version": "22.12.0",
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}